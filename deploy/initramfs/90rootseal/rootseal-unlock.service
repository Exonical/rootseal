[Unit]
Description=Cryptor Network Unlock
DefaultDependencies=no
After=network.target
Before=cryptsetup-pre.target
Wants=network.target

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/bin/rootseal-unlock.sh
TimeoutSec=300
StandardOutput=kmsg
StandardError=kmsg

[Install]
WantedBy=initrd.target
